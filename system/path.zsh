export PATH="./bin:$HOME/bin:$HOME/.rbenv/shims:/usr/local/bin:/usr/local/sbin:$ZSH/bin:$PATH:/opt/local/bin:/usr/local/share/npm/bin"

# using http://postgresapp.com/
export PATH="/Applications/Postgres.app/Contents/MacOS/bin:$PATH"

export MANPATH="/usr/local/man:/usr/local/mysql/man:/usr/local/git/man:$MANPATH"

# add google app engine
export PATH="$HOME/SDKs/google-cloud-sdk/bin:$PATH"

#MacTex El Capitan
export PATH="/Library/TeX/Distributions/Programs/texbin:$PATH"

# PHP Composer
export PATH=$PATH:~/.composer/vendor/bin

# GO
export GOPATH=$HOME/go
export GOROOT=/usr/local/opt/go/libexec
export PATH=$PATH:$GOPATH/bin
export PATH=$PATH:$GOROOT/bin

# Python 2
export PATH=$PATH:$HOME/Library/Python/2.7/bin  
# Python 3
export PATH=$PATH:$HOME/Library/Python/3.7/bin
